{
    "name": "demoniqus/tasks-bundle",
    "description": "Create tasks for application",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Antipov Dmitry",
            "email": "demoniqus@mail.ru"
        }
    ],
    "require": {
        "php": ">=8.2",
        "evrinoma/utils-bundle": "dev-main",
        "evrinoma/dto-bundle": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Demoniqus\\Tasks\\": "src/",
            "Demoniqus\\Tasks\\Tests\\": "tests/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    }
}